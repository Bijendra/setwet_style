<script type="text/javascript">
var videoData = {};
<%
   @data.each do|value|
%>
  videoData['<%= value[3] %>'] = "<%= value[4] %>";
<% end %>



var video_id = "";
// 2. This code loads the IFrame Player API code asynchronously.
var tag = document.createElement('script');
tag.src = "//www.youtube.com/iframe_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

// 3. This function creates an <iframe> (and YouTube player)
    // after the API code downloads.
    var player;
    function onYouTubeIframeAPIReady() {
    if(video_id != "") {
      player = new YT.Player('large_youtube', {
      height: '600',
      width: '750',
      videoId: video_id,
      events: {
        'onReady': onPlayerReady,
        'onStateChange': onPlayerStateChange
      }
      });
    }
    }
    
    // 4. The API will call this function when the video player is ready.
    function onPlayerReady(event) {
      event.target.playVideo();
    }
   
    // 5. The API calls this function when the player's state changes.
    //    The function indicates that when playing a video (state=1),
    //    the player should play for six seconds and then stop.
    var done = false;
    function onPlayerStateChange(event) {
    if((event.data == YT.PlayerState.PLAYING) || (event.data == YT.PlayerState.BUFFERING) || (event.data == YT.PlayerState.CUED)) {
       document.getElementById('large_youtube').style.display = "";    
    } else if(event.data == YT.PlayerState.PAUSED) {
       document.getElementById('large_youtube').style.display = "none";           
    }
     else if(event.data == YT.PlayerState.ENDED){
      document.getElementById('large_youtube').style.display = "none";
      document.getElementById('big_video_div').style.display = "block";     
     }
    }

    function stopVideo() {
      player.stopVideo();
    }
    


function start_video(vid) {
    video_id = vid;
    document.getElementById('large_youtube').style.display = "";
    onYouTubeIframeAPIReady();
    document.getElementById('large_youtube').src = "http://www.youtube.com/embed/"+vid+"?wmode=transparent";
    document.getElementById('big_video_div').style.display = "none";     
    document.getElementById('afterVideoImg').src = videoData[vid];
}

function hide_video() { 
    document.getElementById('big_video_div').style.display = 'none'; 
}

function replayVideo() {
    document.getElementById('big_video_div').style.display = "none"; 
  start_video(video_id);
}










