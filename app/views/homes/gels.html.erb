 <script>
 function start_carousel(id){
if(/coolhead/.test(id)){ $('#myCarousel .carousel-inner .item.active').removeClass('active'); $('#myCarousel .carousel-inner div:nth-child(' + 1 + ')').addClass('active');}
if(/swlook/.test(id)){ $('#myCarousel .carousel-inner .item.active').removeClass('active'); $('#myCarousel .carousel-inner div:nth-child(' + 2 + ')').addClass('active');}
if(/vhold/.test(id)){ $('#myCarousel .carousel-inner .item.active').removeClass('active'); $('#myCarousel .carousel-inner div:nth-child(' + 3 + ')').addClass('active');}
document.getElementById("myCarousel").style.display = "block";
document.getElementById("gel_bg").style.display = "none";
document.getElementById("gel_bg2").style.display = "block";
document.getElementById("gel_text").style.display = "none";
document.getElementById("gel_main_div").style.display = "none";
document.getElementById("youtube_video").style.display = "none";
$('#myCarousel').carousel({
  interval: 3000
});
$('#myCarousel').carousel('cycle');
};


function start_video(){
document.getElementById("gel_bg").style.display = "none";
document.getElementById("gel_bg2").style.display = "block";
document.getElementById("gel_main_div").style.display = "none";
document.getElementById("gel_text").style.display = "none";
document.getElementById("youtube_video").style.display = "none";
$("#btn_cross").css("display","block");
$("#thevideo").css("display","block");
}

function hide_video(state){
 var div = document.getElementById("thevideo");
    var iframe = div.getElementsByTagName("iframe")[0].contentWindow;
    div.style.display = state == 'hide' ? 'none' : '';
    func = state == 'hide' ? 'pauseVideo' : 'playVideo';
    iframe.postMessage('{"event":"command","func":"' + func + '","args":""}', '*');
$("#btn_cross").css("display","none");
document.getElementById("gel_main_div").style.display = "";
document.getElementById("gel_text").style.display = "";
document.getElementById("youtube_video").style.display = "";
document.getElementById("gel_bg").style.display = "";
document.getElementById("gel_bg2").style.display = "none";
}
 </script>
 



